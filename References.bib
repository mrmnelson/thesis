	Isi = {WOS:000169550700018},
	Issn = {0020-1669},
	Journal = {Inorg. Chem.},
	Keywords = {X-ray, Bite angle, Palladium, Allyl Regioselectivity, Allylic alkylation, Xantphos},
	Number = {14},
	Pages = {3363--3372},
	Publication-Type = {J},
