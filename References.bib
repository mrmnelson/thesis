	Keywords = {Rhodium, PCP, Pincer, C-H Activation, C-C Activation, Theoretical},
	Month = {Jul},
	Number = {29},
	Pages = {7095--7104},
	Publication-Type = {J},
	Title = {Selective C-C vs C-H bond activation by rhodium(I) PCP pincer complexes. A computational study},
