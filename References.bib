	Date-Modified = {2012-02-20 17:13:46 +1300},
	Doi = {{10.1039/dt9910002769}},
	Issn = {{0300-9246}},
	Journal = {JOURNAL OF THE CHEMICAL SOCIETY-DALTON TRANSACTIONS},
	Keywords = {Carbaboranes, Platinum, NMR, Coupling constants},
	Month = {{OCT}},
