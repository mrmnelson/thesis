	Issn = {{0002-7863}},
	Journal = {J. Am. Chem. Soc.},
	Keywords = {Protonation, Phosphonium, Basicity,},
	Month = {NOV 20},
	Number = {24},
	Pages = {9185-9192},
	Title = {Heats of protonation of transition-metal complexes - the effect of phosphine basicity on metal basicity in CpIr(CO)(PR3) and Fe(CO)3(PR3)2},
	Unique-Id = {{ISI:A1991GQ93100026}},
