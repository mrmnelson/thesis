	Doi = {10.1021/ja029341y},
	Isi = {WOS:000182682700052},
	Issn = {0002-7863},
	Journal = {J. Am. Chem. Soc.},
	Keywords = {Alcoholysis, Palladium, Acyl, Polymerisation, Xantphos},
	Number = {18},
