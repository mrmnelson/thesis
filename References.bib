	Keywords = {Metathesis, review, Alkane metathesis, Alkene metathesis},
	Number = {1},
	Pages = {42--56},
	Publication-Type = {J},
	Title = {The metathesis reactions: from a historical perspective to recent developments},
	Volume = {29},
	Year = {2005},
