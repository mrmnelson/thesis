	Issn = {0028-0836},
	Journal = {Nature},
	Keywords = {Xantphos},
	Month = {Oct},
	Number = {7011},
	Pages = {966--971},
	Publication-Type = {J},
	Title = {Ultra-remote stereocontrol by conformational communication of information along a carbon chain},
	Volume = {431},
	Year = {2004},
	Bdsk-File-1 = {}}

