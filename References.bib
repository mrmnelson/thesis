	Issn = {0276-7333},
	Journal = {Organometallics},
	Keywords = {N-alkylation, Palladium, Xanthene, Xantphos, Phosphole},
	Month = {Apr},
	Number = {8},
	Pages = {1846--1855},
	Publication-Type = {J},
	Title = {Xanthene-phosphole ligands: Synthesis, coordination chemistry, and activity in the palladium-catalyzed amine allylation},
	Volume = {26},
	Year = {2007},
