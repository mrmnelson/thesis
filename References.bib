	Month = {Jan},
	Number = {1-2},
	Pages = {175--187},
	Publication-Type = {J},
	Title = {Reduction of Alkyl Halides by Triethylsilane Based on a Cationic Iridium Bis(phosphinite) Pincer Catalyst: Scope, Selectivity and Mechanism},
